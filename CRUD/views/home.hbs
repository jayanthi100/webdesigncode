<!DOCTYPE html>
<head>
    <style>

.card1-header {
  background-color: #333;
  width:600px;
  padding: 16px;
  text-align: center;
  position:absolute;
  top:20px;
  left:210px;
}
.card1-header .text-header {
  margin: 0;
  font-size: 18px;
  color:white;
}





    </style>
    </head>
<div class="card1-header">
<h3  class="text-header">STUDENTS PORTAL</h3>
</div>
<p><a href="/adduser" class="addbtn">Add User</a></p>


<table style="zoom: 1.5;right:100px;top:50px;position:relative">
    <thead>
        <tr>
        <td>NAME</td>
        <td>ID</td>
        <td>GENDER</td>
        <td>DEPARTMENT</td>
        <td>NATIONALITY</td>
        <td>EDIT</td>
        <td>DELETE</td>
        <td>VIEW</td>
        </tr>
    </thead>
    <tbody>
 {{#each rows}}
 <tr>
    <td>{{this.NAME}}</td>
    <td>{{this.ID}}</td>
    <td>{{this.GENDER}}</td>
    <td>{{this.DEPARTMENT}}</td>
    <td>{{this.NATIONALITY}}</td>
    <td><a class="edit" href="/edituser/{{this.ID}}">Edit</a></td>
    <td><a onclick="return confirm('Are you sure?')" class="delete" href="/deleteuser/{{this.ID}}">Delete</a></td>
    <td><a class="vie" href="/viewuser/{{this.ID}}">View</a></td>
    </tr>
{{/each}}
</tbody>
</table>






